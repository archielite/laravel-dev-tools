{
    "name": "archielite/laravel-dev-tools",
    "description": "Laravel Development Tools",
    "keywords": [
        "archi elite",
        "php",
        "laravel",
        "laravel-dev-tools"
    ],
    "authors": [
        {
            "name": "Archi Elite",
            "email": "contact@archielite.com"
        }
    ],
    "homepage": "https://github.com/archielite/laravel-dev-tools",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.49|^10.0",
        "phpstan/phpdoc-parser": "^1.15",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "laravel/pint": "^0.2.4",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.6",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ArchiElite\\LaravelDevTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArchiElite\\LaravelDevTools\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArchiElite\\LaravelDevTools\\LaravelDevToolsServiceProvider"
            ]
        }
    }
}
